<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/music2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.EverGreenMusicApp"
        tools:targetApi="31">
        <activity
            android:name=".AlbumActivity"
            android:label="@string/albums"
            android:exported="false" />
        <activity
            android:name=".ArtistActivity"
            android:label="@string/artists"
            android:exported="false" />
        <activity
            android:name=".PlayListActivity"
            android:exported="false"
            android:label="@string/playing" />
        <activity
            android:name=".SongsActivity"
            android:exported="false"
            android:label="@string/song" />
        <activity
            android:name=".MusicActivity"
            android:exported="false"
            android:label="@string/music" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>